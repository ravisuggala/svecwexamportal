								<TD  WIDTH="20%" ALIGN="right">Regd No:</TD>
								<td WIDTH="10%" ALIGN="right"><input maxlength="10" name="regdno"></td>
							
							<TR>
							    <TD colspan=2 align="center">
							    <INPUT TYPE="SUBMIT" NAME="submit" VALUE="Submit"></INPUT></TD>

							</TR>
							</FORM>
						</TABLE>
